            <bind id="info_own" nodeset="/iics_survey_school_questionnaire_v1/info_own" relevant="/iics_survey_school_questionnaire_v1/stoves = 'own'" />
            <bind id="quantity_construction" nodeset="/iics_survey_school_questionnaire_v1/info_own/quantity_construction" type="int" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'own'" />
            <bind id="pan_size_construction" nodeset="/iics_survey_school_questionnaire_v1/info_own/pan_size_construction" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'own'" />
            <bind id="rings_construction" nodeset="/iics_survey_school_questionnaire_v1/info_own/rings_construction" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'own'" />
            <bind id="fuel_construction" nodeset="/iics_survey_school_questionnaire_v1/info_own/fuel_construction" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'own'" />
            <bind id="improved_stoves" nodeset="/iics_survey_school_questionnaire_v1/improved_stoves" type="string" />
