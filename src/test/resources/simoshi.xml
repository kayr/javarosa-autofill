    <h:body class="pages">
        <select1 ref="/iics_survey_school_questionnaire_v1/visit">
            <label>1. Type of visit</label>
            <item>
                <label>School Baseline Visit</label>
                <value>baseline</value>
            </item>
            <item>
                <label>School Information Update</label>
                <value>update</value>
            </item>
        </select1>
        <group ref="/iics_survey_school_questionnaire_v1/school" appearance="field-list">
            <label>2. </label>
            <input ref="/iics_survey_school_questionnaire_v1/school/school_prompt" appearance="ex:org.odk.collect.android.activities.entities.PrefillActivity(table_name='school',returnFld='__code;school_name')">
                <label>2.1.  Select the school Name</label>
                <hint>Select name of the school you are visiting.</hint>
            </input>
        </group>
        <group appearance="field-list">
            <label></label>
            <input ref="/iics_survey_school_questionnaire_v1/visit_date">
                <label>3. Date of Visit</label>
            </input>
            <input ref="/iics_survey_school_questionnaire_v1/interviewer">
                <label>4. Name Name of Interviewer</label>
            </input>
            <input ref="/iics_survey_school_questionnaire_v1/interviewer_mobile">
                <label>5.  Mobile Number</label>
            </input>
        </group>
        <group appearance="field-list">
            <label></label>
            <input ref="/iics_survey_school_questionnaire_v1/school_name">
                <label>6. School Name:</label>
            </input>
            <input ref="/iics_survey_school_questionnaire_v1/address">
                <label>7. School Address:</label>
            </input>
            <input ref="/iics_survey_school_questionnaire_v1/phone">
                <label>8. School Telephone</label>
            </input>
            <input ref="/iics_survey_school_questionnaire_v1/email">
                <label>9. Email</label>
            </input>
        </group>
        <select1 ref="/iics_survey_school_questionnaire_v1/district" appearance="minimal w4">
            <label>10. District</label>
            <item>
                <label>Jinja</label>
                <value>jinja</value>
            </item>
            <item>
                <label>Kampala</label>
                <value>kampala</value>
            </item>
            <item>
                <label>Mukono</label>
                <value>mukono</value>
            </item>
            <item>
                <label>Wakiso</label>
                <value>wakiso</value>
            </item>
        </select1>
        <select1 ref="/iics_survey_school_questionnaire_v1/subcounty" appearance="minimal w2">
            <label>11. Subcounty</label>
            <itemset nodeset="instance('subcounty')/dynamiclist/item[@parent=/iics_survey_school_questionnaire_v1/district]">
                <value ref="value" />
                <label ref="label" />
            </itemset>
        </select1>
        <group appearance="field-list">
            <label></label>
            <input ref="/iics_survey_school_questionnaire_v1/headteacher">
