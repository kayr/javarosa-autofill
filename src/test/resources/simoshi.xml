            <bind id="visit" nodeset="/iics_survey_school_questionnaire_v1/visit" type="string" required="true()" />
            <bind id="school" nodeset="/iics_survey_school_questionnaire_v1/school" relevant="/iics_survey_school_questionnaire_v1/visit = 'update'" />
            <bind id="school_prompt" nodeset="/iics_survey_school_questionnaire_v1/school/school_prompt" type="string" required="true()" />
            <bind id="__code" nodeset="/iics_survey_school_questionnaire_v1/school/__code" type="string" readonly="true()" />
            <bind id="visit_date" nodeset="/iics_survey_school_questionnaire_v1/visit_date" type="date" required="true()" />
            <bind id="interviewer" nodeset="/iics_survey_school_questionnaire_v1/interviewer" type="string" required="true()" />
            <bind id="interviewer_mobile" nodeset="/iics_survey_school_questionnaire_v1/interviewer_mobile" type="string" required="true()" constraint="regex(., &quot;^07[0-9]{8}$&quot;)" jr:constraintMsg="&quot;Please enter a valid phone number. e.g. 0778345678&quot;" />
            <bind id="school_name" nodeset="/iics_survey_school_questionnaire_v1/school_name" type="string" required="true()" />
            <bind id="address" nodeset="/iics_survey_school_questionnaire_v1/address" type="string" required="true()" />
            <bind id="phone" nodeset="/iics_survey_school_questionnaire_v1/phone" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" constraint="regex(., &quot;^[0-9]{10}$&quot;)" jr:constraintMsg="&quot;Please enter a valid phone number. e.g. 0778345678&quot;" />
            <bind id="email" nodeset="/iics_survey_school_questionnaire_v1/email" type="string" relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" constraint="regex(., '[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,5}')" jr:constraintMsg="put a valid email" />
            <bind id="district" nodeset="/iics_survey_school_questionnaire_v1/district" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" />
            <bind id="subcounty" nodeset="/iics_survey_school_questionnaire_v1/subcounty" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/visit = 'baseline'" />
