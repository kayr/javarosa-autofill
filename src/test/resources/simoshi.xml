            <bind id="fuel_stone" nodeset="/iics_survey_school_questionnaire_v1/info_stone/fuel_stone" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'stone'" />
            <bind id="info_improved" nodeset="/iics_survey_school_questionnaire_v1/info_improved" relevant="/iics_survey_school_questionnaire_v1/stoves = 'improved'" />
            <bind id="quantity_improved" nodeset="/iics_survey_school_questionnaire_v1/info_improved/quantity_improved" type="int" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'improved'" />
            <bind id="pan_size_improved" nodeset="/iics_survey_school_questionnaire_v1/info_improved/pan_size_improved" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/stoves = 'improved'" />
