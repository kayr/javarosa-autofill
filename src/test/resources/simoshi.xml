            <bind id="director_phone" nodeset="/iics_survey_school_questionnaire_v1/director_phone" type="string" required="true()" constraint="regex(., &quot;^07[0-9]{8}$&quot;)" jr:constraintMsg="&quot;Please enter a valid phone number. e.g. 0778345678&quot;" />
            <bind id="school_level" nodeset="/iics_survey_school_questionnaire_v1/school_level" type="string" required="true()" />
