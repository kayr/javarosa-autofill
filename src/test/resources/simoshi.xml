            <bind id="manufacturer" nodeset="/iics_survey_school_questionnaire_v1/improved_stove_details/manufacturer" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/improved_stoves = 'yes'" />
            <bind id="ics_size" nodeset="/iics_survey_school_questionnaire_v1/improved_stove_details/ics_size" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/improved_stoves = 'yes'" />
            <bind id="ics_ring" nodeset="/iics_survey_school_questionnaire_v1/improved_stove_details/ics_ring" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/improved_stoves = 'yes'" />
            <bind id="ics_condition" nodeset="/iics_survey_school_questionnaire_v1/improved_stove_details/ics_condition" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/improved_stoves = 'yes'" />
            <bind id="ics_repair" nodeset="/iics_survey_school_questionnaire_v1/improved_stove_details/ics_repair" type="string" required="true()" relevant="/iics_survey_school_questionnaire_v1/improved_stoves = 'yes'" />
            <bind id="cook_holiday" nodeset="/iics_survey_school_questionnaire_v1/cook_holiday" type="string" required="true()" />
            <bind id="cook_special" nodeset="/iics_survey_school_questionnaire_v1/cook_special" type="string" required="true()" />
