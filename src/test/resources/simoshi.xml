            <bind id="boarding" nodeset="/iics_survey_school_questionnaire_v1/enrolment/boarding" type="int" required="true()" />
            <bind id="day" nodeset="/iics_survey_school_questionnaire_v1/enrolment/day" type="int" required="true()" />
            <bind id="teacher_no" nodeset="/iics_survey_school_questionnaire_v1/enrolment/teacher_no" type="int" required="true()" />
            <bind id="non_teacher_no" nodeset="/iics_survey_school_questionnaire_v1/enrolment/non_teacher_no" type="int" required="true()" />
            <bind id="totals" nodeset="/iics_survey_school_questionnaire_v1/totals" />
            <bind id="total_enrolment" nodeset="/iics_survey_school_questionnaire_v1/totals/total_enrolment" type="int" required="true()" readonly="true()" calculate="/iics_survey_school_questionnaire_v1/enrolment/boarding + /iics_survey_school_questionnaire_v1/enrolment/day" />
            <bind id="total_traff" nodeset="/iics_survey_school_questionnaire_v1/totals/total_traff" type="int" required="true()" readonly="true()" calculate="/iics_survey_school_questionnaire_v1/enrolment/non_teacher_no + /iics_survey_school_questionnaire_v1/enrolment/teacher_no" />
